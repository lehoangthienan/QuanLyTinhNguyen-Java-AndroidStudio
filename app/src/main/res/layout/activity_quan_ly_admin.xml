<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/anhnen"
    tools:context="bigc.uit.quanlytinhnguyen.QuanLyAdminActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/btnThemHoatDongTinhNguyen"
        android:background="@drawable/drawimagebuttonadmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/add"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.351" />

    <ImageButton
        android:id="@+id/btnSuaHoatDongTinhNguyen"
        android:background="@drawable/drawimagebuttonadmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/update"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/btnThemHoatDongTinhNguyen"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.51" />

    <ImageButton
        android:id="@+id/btnXoaHoatDongTinhNguyen"
        android:background="@drawable/drawimagebuttonadmin"
        android:layout_width="wrap_content"
